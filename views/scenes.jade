div.card
  div.card-content    
    span.card-title.black-text My Scenes
    div.row
      div.col.m12.s12
        ul.collection
          each s, index in scenes
            li.collection-item.scene(data-scene='#{JSON.stringify(s)}', data-index='#{index}')
              div= s.title
                a.preview-scene.modal-trigger(href='#scenePreview' onClick="getScript(#{JSON.stringify(s)})" title="Preview Scene")
                  i.material-icons play_arrow
script(type='text/javascript' src='/jsmol/JSmol.min.js')
script(src='/javascripts/scene.js')
